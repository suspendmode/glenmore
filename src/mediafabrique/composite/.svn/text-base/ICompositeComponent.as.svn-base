package patterns.structural.composite {
  import flash.utils.Dictionary;
  
  /**
   *
   * @author piku
   *
   */
  public interface ICompositeComponent extends IComponent, IComponentList {    
    
    /**
     *
     * @return
     *
     */
    function toArray() : Array;

    /**
     *
     * @param value
     *
     */
    function set map(value : Dictionary) : void;
    
    /**
     *
     * @return
     *
     */
    function get map() : Dictionary;
    
    
}
}