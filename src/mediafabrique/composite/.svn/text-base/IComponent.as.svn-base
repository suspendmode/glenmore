package patterns.structural.composite {
  import flash.events.IEventDispatcher;
  
  import patterns.concurrency.event.ICompoundEventDispatcher;
  
  /**
   *
   * @author piku
   *
   */
  public interface IComponent extends ICompoundEventDispatcher {
    
    /**
     *
     * @param value
     *
     */
    function set id(value : String) : void;
    
    /**
     *
     * @return
     *
     */
    function get id() : String;
    
    /**
     *
     * @param value
     *
     */
    function set parent(value : ICompositeComponent) : void;
    
    /**
     *
     * @return
     *
     */
    function get parent() : ICompositeComponent;
  }
}