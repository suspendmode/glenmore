package context {
  import logging.ILogger;
  import logging.Log;
  
  /**
   *
   * @author Peter
   *
   */
  internal final class RootContext extends Context {
    
    /**
     * 
     */
    private static var logger : ILogger = Log.getLogger(RootContext);
    
    /**
     *
     *
     */
    public function RootContext() {
      id = "/";
      super();
      logger.info("{0} root context initialized", this);
    }
  }
}